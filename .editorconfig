# https://editorconfig.org
root = true

[*]
charset = utf-8
indent_size = 2
indent_style = space
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
quote_type = single
spaces_around_operators = true

[*.css]
quote_type = single

[*.md]
insert_final_newline = false
trim_trailing_whitespace = false